{{>layouts/header}}

<table class="table">
    <thead>
    <tr>
        <th scope="col">id</th>
        <th scope="col">Title</th>
        <th scope="col">content</th>
        <th scope="col">text</th>

    </tr>
    </thead>
    <tbody>
    {{#articleList}}
<!--불러오는 데이터가 리스트일 경우에는 반복이된다.-->
        <tr>
            <th>{{id}}</th>
            <th ><a href="/articles/{{id}}">{{title}}</a></th>
            <td>{{content}}</td>
            <td>{{text}}</td>
        </tr>
    {{/articleList}}
    </tbody>
</table>
<a href="/articles/new">New Article</a>
{{>layouts/footer}}