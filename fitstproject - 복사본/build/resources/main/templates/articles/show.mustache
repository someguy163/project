{{>layouts/header}}

<table class="table">
    <thead>
    <tr>
        <th scope="col">id</th>
        <th scope="col">Title</th>
        <th scope="col">content</th>
        <th scope="col">text</th>

    </tr>
    </thead>
    <tbody>
    {{#article}}
    <tr>
        <th>{{id}}</th>
        <th >{{title}}</th>
        <td>{{content}}</td>
        <td>{{text}}</td>
    </tr>
    {{/article}}
    </tbody>
</table>
<div class="container">
<a href="/articles/{{article.id}}/edit" class="btn btn-primary">Edit</a>
<a href="/articles" class="btn btn-primary"> Go to ArticleList</a>
    <a href="/articles/{{article.id}}/delete" class="btn btn-danger">Delete</a>
</div>
{{>layouts/footer}}